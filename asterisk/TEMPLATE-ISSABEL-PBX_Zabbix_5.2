            <discovery_rules>
                <discovery_rule>
                    <name>Discovery IAX2 Peers</name>
                    <key>discovery.asterisk[discovery.iax2.peers]</key>
                    <delay>30m</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>IAX2 peer {#PEER} latency</name>
